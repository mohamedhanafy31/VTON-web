{
  "name": "vton-web",
  "version": "1.0.0",
  "description": "Virtual Try-On Web Application",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "cp env.development.template .env 2>/dev/null || true && node src/server.js",
    "prod": "cp env.production.template .env 2>/dev/null || true && node src/server.js",
    "start:dev": "NODE_ENV=development node src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "setup:dev": "echo 'Setting up development environment...' && cp env.development.template .env && echo 'Please update .env with your development Firebase credentials'",
    "setup:prod": "echo 'Setting up production environment...' && cp env.production.template .env && echo 'Please update .env with your production Firebase credentials'",
    "validate-config": "node src/utils/validateConfig.js"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^3.0.0",
    "bcrypt": "^5.1.0",
    "busboy": "^1.6.0",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^13.5.0",
    "memorystore": "^1.6.7",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "keywords": [
    "virtual-try-on",
    "ai",
    "fashion",
    "ecommerce"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  }
}
